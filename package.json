{
  "homepage": "https://dstorey87.github.io/darrenstorey",
  "name": "developerfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "colorthief": "latest",
    "gh-pages": "latest",
    "jest-canvas-mock": "latest",
    "lottie-react": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-easy-emoji": "latest",
    "react-headroom": "latest",
    "react-scripts": "latest",
    "react-twitter-embed": "latest",
    "sass": "latest",
    "copyfiles": "latest"
  },
  "scripts": {
    "predeploy": "npm run build-docs",
    "start": "set NODE_PATH=C:/artifacts/node_modules && node fetch.js && C:/artifacts/node_modules/.bin/react-scripts start",
    "build": "set NODE_PATH=C:/artifacts/node_modules && node fetch.js && C:/artifacts/node_modules/.bin/react-scripts build",
    "build-docs": "set NODE_PATH=C:/artifacts/node_modules && node fetch.js && C:/artifacts/node_modules/.bin/react-scripts build && C:/artifacts/node_modules/.bin/copyfiles -u 1 build/**/* docs/ && rmdir /s /q build",
    "deploy": "gh-pages -b main -d docs",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "C:/artifacts/node_modules/.bin/prettier --write \"./**/*.{js,css,json,scss}\"",
    "check-format": "C:/artifacts/node_modules/.bin/prettier -c \"./**/*.{js,css,json,scss}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "latest",
    "dotenv": "latest",
    "prettier": "latest"
  }
